@model IEnumerable<Anadolu.Entitiess.Bayiler>



<thead>
    <tr style="background-color:cadetblue">

        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Telefon)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fax)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Adres)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>


        <th> İnsert işlemleri
        </th>

    </tr>
</thead>

<tbody>

    @foreach (var item in Model)
    {


        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Telefon)
                (Tel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fax)
                (Fax)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Adres)

            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>

        <td>
            <a href="/Admin/BayiDelete/@item.Id" class="btn btn-danger">Sil</a>





            <a href="/Admin/BayiEdit/@item.Id" class="btn btn-success">Düzenle</a>

        </td>

        </tr>
    }

</tbody>



